{
  "name": "programming-svg-icons",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "generate-icons": "svg-to-ts",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "svg-to-ts": {
    "conversionType": "constants",
    "srcFiles": [
      "./projects/programming-svg-icons/icons/**/*.svg"
    ],
    "outputDirectory": "./projects/programming-svg-icons/src/lib",
    "interfaceName": "ProgrammingSvgIcons",
    "typeName": "programmingSvgIcons",
    "prefix": "",
    "svgoConfig": {
      "plugins": [
        {
          "cleanupAttrs": true
        }
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "~12.1.2",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.4",
    "@angular-extensions/svg-icons-builder": "^1.1.0",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.0",
    "svg-to-ts": "^6.0.1",
    "typescript": "~4.3.2"
  }
}
